---

interface Props {
  title: string
  url?: string
  math?: boolean
  bokeh?: boolean
}
const { title, url, math, bokeh } = Astro.props;
---
<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
<link rel="sitemap" href="/sitemap-index.xml">
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1">
<title>{title}</title>

{bokeh && (
  <script src="https://cdn.bokeh.org/bokeh/release/bokeh-3.8.0.min.js" is:inline ></script>
  <script src="https://cdn.bokeh.org/bokeh/release/bokeh-gl-3.8.0.min.js" is:inline ></script>
  <script src="https://cdn.bokeh.org/bokeh/release/bokeh-widgets-3.8.0.min.js" is:inline ></script>
  <script src="https://cdn.bokeh.org/bokeh/release/bokeh-tables-3.8.0.min.js" is:inline ></script>
)}

<!-- Web analytics -->
{import.meta.env.PROD && (
  <script src="https://cdn.counter.dev/script.js" data-id="3c4649ad-19ea-4bce-8d9d-cee11202c82a" data-utcoffset="-4"></script>
)}
